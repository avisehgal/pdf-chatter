{
    "projects": [
      {
        "name": "frontend",
        "root": "frontend",
        "buildCommand": "npm run build",
        "devCommand": "npm run dev",
        "outputDirectory": "out",
        "framework": "nextjs"
      },
      {
        "name": "backend",
        "root": "backend",
        "buildCommand": "pip install -r requirements.txt",
        "devCommand": "uvicorn main:app --host 0.0.0.0 --port 8000",
        "outputDirectory": "."
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/main.py"
      },
      {
        "src": "(.*)",
        "dest": "/frontend/$1"
      }
    ]
  }
  